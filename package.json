    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/moment": "^1.6.1",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/vuetify": "^1.12.3",
    "dotenv": "^16.0.3",
    "nuxt": "^2.15.8",
    "vue": "^2.7.10",
    "vue-numeral-filter": "^2.2.0",
    "vue-server-renderer": "^2.7.10",
    "vue-sweetalert2": "^5.0.5",
    "vue-template-compiler": "^2.7.10",
    "vuetify": "^2.6.10"
  },
  "devDependencies": {
    "push-dir": "^0.4.1"
  }
